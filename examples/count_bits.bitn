# Count set bits example using Nim-style syntax

proc count_set_bits(): u32 =
  let value: u32 = 0xFF
  let count: u32 = 0
  return count

proc simple_return(): u32 =
  return 42

proc nested_operations(): u32 =
  let x: u32 = 10
  let y: u32 = 20
  let z: u32 = add(x, y)
  return z

proc main(): u32 =
  let result1: u32 = simple_return()
  let result2: u32 = nested_operations()
  let result3: u32 = count_set_bits()
  let final: u32 = add(result1, result2)
  return final
